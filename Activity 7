using System;
using System.Linq;

using System;
using System.Collections.Generic;


class ProductSearch
{
    static void Main()
    {
        List<string> products = new List<string>();

        
        Console.WriteLine("Enter 8 product names:");
        for (int i = 0; i < 8; i++)
        {
            Console.Write($"Product {i + 1}: ");
            products.Add(Console.ReadLine());
        }

      
        Console.Write("\nEnter a search keyword: ");
        string keyword = Console.ReadLine().ToLower();

        
        List<string> matches = new List<string>();
        foreach (string product in products)
        {
            if (product.ToLower().Contains(keyword))
            {
                matches.Add(product);
            }
        }

        
        if (matches.Count > 0)
        {
            Console.WriteLine("\nMatching products:");
            foreach (string match in matches)
            {
                Console.WriteLine(match);
            }
        }
        else
        {
            Console.WriteLine("\nNo products found.");
        }
    }
}
